    "clean": "rm -rf dist"
  },
  "dependencies": {
    "spotify-web-api-js": "^1.5.2"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.3.0",
    "@types/spotify-web-playback-sdk": "^0.1.19",
    "react": "^18.3.1",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2"
  }
}
